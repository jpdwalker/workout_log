= simple_form_for(@workout, html: { class: 'form-horizontal' }) do |f|
  = f.input :date, label: "Date"
  = f.input :workout, label: "What did you work?", input_html: { class: "form-control" }
  = f.input :mood, label: "How are you feeling?", input_html: { class: "form-control"}
  = f.input :length, label: "How long was the workout?", input_html: { class: "form-control"}
  %br/
  = f.button :submit
